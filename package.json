{
  "name": "gramera",
  "version": "1.0.0",
  "description": "Full Stack Gramera Social Media App",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "dev-mock": "nodemon server-mock.js",
    "start": "node server.js",
    "test-db": "node test-connection.js",
