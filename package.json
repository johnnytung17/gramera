    "@sendgrid/mail": "^8.1.5",
    "aws-sdk": "^2.1095.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.1.7",
    "multer": "^2.0.1",
